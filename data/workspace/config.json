{
  "name": "AI Agent Configuration",
  "version": "1.0.0",
  "tools": [
    {
      "name": "file_system",
      "enabled": true,
      "permissions": ["read", "write", "list", "search"]
    }
  ],
  "security": {
    "safe_mode": true,
    "allowed_paths": ["data/workspace", "data/temp"],
    "max_file_size": "10MB"
  },
  "features": {
    "backup_enabled": true,
    "monitoring_enabled": true,
    "error_handling": "comprehensive"
  }
}
