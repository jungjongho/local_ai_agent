# 환경 설정
ENVIRONMENT=development
DEBUG=True

# API 설정
OPENAI_API_KEY=your_openai_api_key_here
API_V1_STR=/api/v1

# 서버 설정
HOST=0.0.0.0
PORT=8000

# 데이터베이스
DATABASE_URL=sqlite:///./local_ai_agent.db

# 프로젝트 경로
WORKSPACE_PATH=../workspace/generated_projects
TEMPLATES_PATH=./templates

# CORS 설정
BACKEND_CORS_ORIGINS=["http://localhost:3000", "http://localhost:3001"]

# WebSocket 설정
WS_HOST=localhost
WS_PORT=8001

# 로깅
LOG_LEVEL=INFO
